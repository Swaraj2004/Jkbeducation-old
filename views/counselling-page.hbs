  <div id="contact-popup">
    <span id="contact-head">Contact Us</span>
    <span id="cross">&times;</span>
    <form id="contact-form">
      <div>
        <label>Name</label><br />
        <input type="text" placeholder="Click here to enter Name" name="name" />
      </div>
      <div>
        <label>Phone Number</label><br />
        <input type="number" placeholder="Click here to enter Contact Number" name="phone" />
      </div>
      <div>
        <label>Email ID</label><br />
        <input type="email" placeholder="Click here to enter Email" name="email" />
      </div>
      <div>
        <label>Address</label><br />
        <input type="text" placeholder="Click here to enter Address" name="address" />
      </div>
      <div>
        <label>Purpose</label><br />
        <input type="text" placeholder="Click here to enter Purpose" name="purpose" />
      </div>
      <div>
        <button type="submit">Submit Details</button>
      </div>
      <div></div>
    </form>
  </div>
</div>
