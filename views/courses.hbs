  <div id="contact-popup">
    <span id="contact-head">Contact Us</span>
    <span id="cross">&times;</span>
    <form action="" id="contact-form">
      <div>
        <label>Name</label><br />
        <input type="text" placeholder="Click here to enter Name" class="contact-input" />
      </div>
      <div>
        <label>Phone Number</label><br />
        <input type="text" placeholder="Click here to enter Contact Number" class="contact-input" />
      </div>
      <div>
        <label>Email ID</label><br />
        <input type="email" placeholder="Click here to enter Email" class="contact-input" />
      </div>
      <div>
        <label>Address</label><br />
        <input type="text" placeholder="Click here to enter Address" class="contact-input" />
      </div>
      <div>
        <label>Purpose</label><br />
        <input type="text" placeholder="Click here to enter Purpose" class="contact-input" />
      </div>
      <div>
        <button>Submit Details</button>
      </div>
    </form>
  </div>
</div>
